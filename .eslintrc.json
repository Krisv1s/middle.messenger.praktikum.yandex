{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-syntax": 0,
    "no-console": 0,
    "guard-for-in": 0,
    "no-continue": 0,
    "consistent-return": 0,
    "no-use-before-define": 0
  }
}
